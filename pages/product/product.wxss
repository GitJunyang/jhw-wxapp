page {background: #f8f8f8;}
/* 搜索 */
.searchFixed{height: 15.2vw;z-index:11;position:relative;}
.search-icon{width:40rpx;height:40rpx !important;float:right;position:absolute;left:52rpx;top:40rpx;}
.search-box{font-size:12px;padding:30rpx 34rpx 27rpx 34rpx;background:#fff;position:fixed;left:0;top:0;width:682rpx;}
.search-box input{border:1px solid #ebebeb;border-radius:60rpx;height:60rpx;line-height: 60rpx; padding-left: 65rpx;}
/** 产品列表 **/
.goods{display:flex;flex-wrap:wrap;justify-content:space-between;padding: 23rpx 23rpx 2vh 23rpx}
.goods .item{display:flex;flex-flow: column;width:341rpx;box-sizing:border-box; margin:13rpx 0 16rpx 0;
background: #fff}
.goods .item_grow{overflow:hidden;width: 100%}
.goods .item_pic{flex: 1;}
.goods .item_content{height: 49px;flex: 0 0 49px; }
.goods image{width: 100%;max-height: 100%;border:1px solid #eee;box-sizing:border-box;}
.goods .pro_title{font-size:14px;color:#000;padding:5px 5px 0 5px;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.goods .pro_price{padding:5px;font-size:13px;color:#ff0000;box-sizing:border-box;font-family:Helvetica}
.goods .unit{font-size:10px;}
/** 回到顶部 **/
.top{position: fixed;bottom: 4vh;right: 4vw; width: 30px;height: 30px;padding: 1vw}
